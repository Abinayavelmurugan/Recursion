public class Main
{
    static int fun(int n){
        int count=0;
        if(n<9){
            return count;
        }
        if(n%10==0){
            count++;
        }
       return count+fun(n/10);
        
    }
	public static void main(String[] args) {
		System.out.println(fun(10110100));
	}
}
